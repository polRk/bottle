{
  "name": "notes-in-bottle",
  "version": "1.0.0",
  "description": "Your notes in bottle",
  "main": "index.js",
  "scripts": {
    "develop": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "start": "parcel lib/index.html -p 3000",
    "build": "npm run type-check && parcel build ./lib/index.html --public-url '/'",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc -w --noEmit"
  },
  "author": "Vladislav Polyakov",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "autoprefixer": "^9.4.10",
    "concurrently": "4.1.0",
    "husky": "1.3.1",
    "jest": "^24.3.1",
    "parcel-bundler": "^1.12.0",
    "prettier": "1.16.4",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettify && npm run test"
    }
  },
  "engines": {
    "node": ">= 10.12.0",
    "npm": ">= 6.9.0",
    "yarn": ">= 1.10.1"
  }
}
